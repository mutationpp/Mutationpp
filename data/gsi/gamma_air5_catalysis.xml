<gsi gsi_mechanism = "gamma">

    <surface_properties name = "none" >
    </surface_properties>
    
    <diffusion_model model="first_order">
    </diffusion_model>

    <production_terms>
        <surface_chemistry>
            <!-- 1 -->
            <reaction formula="N + N => N2">
                <gamma_const> N:0.01 </gamma_const>
            </reaction>
            <!-- 2 -->
            <reaction formula="O + O => O2">
                <gamma_const> O:0.02 </gamma_const>
            </reaction>
            <!-- 3 -->
            <reaction formula="N + O => NO">
                <gamma_const> N:0.03 O:0.04 </gamma_const>
            </reaction> 
        </surface_chemistry>
    </production_terms>
    
</gsi>

